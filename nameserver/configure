    dirs="/usr/unsupported/include /usr/local/include  /usr/sww/include /usr/include/tcl ../tcl $INCLUDE_PATH"
    for i in $dirs ; do
	if test -r $i/tcl.h; then
	    TCL_INCLUDES="-I$i"
	fi
    done
