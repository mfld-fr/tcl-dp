  if test -n "$x_ok"; then
  ac_cv_prog_x_ok="$x_ok" # Let the user override the test.
else
  IFS="${IFS= 	}"; ac_save_ifs="$IFS"; IFS="${IFS}:"
  for ac_dir in $PATH; do
    test -z "$ac_dir" && ac_dir=.
    if test -f $ac_dir/$ac_word; then
      ac_cv_prog_x_ok="1"
      break
    fi
  done
  IFS="$ac_save_ifs"
  test -z "$ac_cv_prog_x_ok" && ac_cv_prog_x_ok="0"
fi
fi
x_ok="$ac_cv_prog_x_ok"
if test -n "$x_ok"; then
  echo "$ac_t""$x_ok" 1>&4
else
  echo "$ac_t""no" 1>&4
fi

if test $x_ok = 1; then
